<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hud.mapper.UserMapper">


<select id="login" resultType="com.hud.model.UserVo">
select uid,upw,uname 
from tb1_user where uid=#{uid} 
and upw=#{upw}


</select>

</mapper>








